<?xml version="1.0" encoding="UTF-8"?>

<manifest xmlns:tools="http://schemas.android.com/tools"
    package="symptomsme.symptomsme.empsoft.projeto.symptomsme"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <application
    android:theme="@style/AppTheme"
    android:supportsRtl="true"
    android:label="@string/app_name"
    android:icon="@drawable/vdoc"
    android:allowBackup="true">

        <activity
            android:name="com.pistis.vdoc.base_activities.SplashScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:noHistory="true"
            android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

    <activity
        android:theme="@style/AppTheme"
        android:label="@string/app_name"
        android:name="com.pistis.vdoc.activities.SintomaActivity"
        tools:ignore="Instantiatable">


    <!--<intent-filter>

    <action android:name="android.intent.action.MAIN"/>

    <category android:name="android.intent.category.LAUNCHER"/>

</intent-filter>-->

</activity>

    <activity
        android:theme="@style/AppTheme.NoActionBar"
        android:label="@string/app_name"
        android:name="com.pistis.vdoc.activities.DiagnosticoActivity"
        android:parentActivityName="com.pistis.vdoc.activities.SintomaActivity"
        tools:ignore="Instantiatable">

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.pistis.vdoc.activities.SintomaActivity" />
    </activity>

    <activity
        android:theme="@style/AppTheme"
        android:label="@string/title_activity_doencas"
        android:name="com.pistis.vdoc.activities.DoencasActivity"
        android:parentActivityName="com.pistis.vdoc.activities.SintomaActivity"
        tools:ignore="Instantiatable" />

    <activity
        android:theme="@style/AppTheme.NoActionBar"
        android:label="@string/title_activity_medicos"
        android:name="com.pistis.vdoc.activities.MedicosActivity"
        android:parentActivityName="com.pistis.vdoc.activities.SintomaActivity"
        tools:ignore="Instantiatable" />

    <activity
        android:theme="@style/AppTheme.NoActionBar"
        android:label="@string/title_activity_medico"
        android:name="com.pistis.vdoc.activities.MedicoActivity"
        android:parentActivityName="com.pistis.vdoc.activities.SintomaActivity"
        tools:ignore="Instantiatable" />

</application>

</manifest>































